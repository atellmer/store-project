@import 'nib'

.likearts-content .section--2
	min-height 670px
	background-color #fafafa
	margin 60px 0
	@media screen and (max-width: $phone)
		min-height 0
		
.likearts-content .section--2 .column--left
	width 60%
	@media screen and (max-width: 992px)
		width 100%
	
.likearts-content .section--2 .column--right
	width 40%
	@media screen and (max-width: 992px)
		width 100%